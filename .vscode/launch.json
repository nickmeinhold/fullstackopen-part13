{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Blogs Server",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/blogs/index.js",
      "cwd": "${workspaceFolder}/blogs",
      "restart": true,
      "runtimeExecutable": "node",
      "runtimeArgs": ["--watch"],
      "console": "integratedTerminal",
      "env": {
        "NODE_ENV": "development"
      }
    }
  ]
}
